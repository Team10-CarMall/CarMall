<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.team10.settings.mapper.ExceptionLogMapper">

    <insert id="addExceptionHandlerLog">
        insert into t_log_exception(
            content,
            create_time,
            user_id
        )
        values(
            #{content},
            #{createTime},
            #{userId}
        )
    </insert>
</mapper>