<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.team10.settings.mapper.DicTypeMapper">
    <select id="getGoodsType" parameterType="java.lang.Boolean" resultType="com.team10.settings.model.DicType">
        select
            dt.id,
            dt.text,
            dt.value,
            dt.order,
            dt.picUrl
        from
            t_dic_type dt
        where
            dt.enable = #{enable}
    </select>
</mapper>